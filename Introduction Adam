//Adam Lebrigui
//Enora Georgeault
var prenom = "Adam";
var nom = "Lebrigui";
var salutation = "Bonjour, mon nom est " + prenom + " " + nom + "!";
var age = "21";
var discipline = "analytique d'affaires";
var informations = "J'ai " + age + " ans et j'étudie en " + discipline + ".";
var introduction = salutation + "\n" + informations;
print(introduction);

//Alternative

var prenom = "Enora";
var nom = "Georgeault";
var salutation = "Bonjour, mon nom est " + prenom + " " + nom + "!";
var age = "21";
var discipline = "analytique d'affaires";
var informations = "J'ai " + age + " ans et j'étudie en " + discipline + ".";
var introduction = salutation + "\n" + informations;
print(introduction);